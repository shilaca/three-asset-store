{
  "name": "@shilaca/three-asset-store",
  "version": "0.0.8",
  "description": "loading and saving assets",
  "main": "./dist/assetStore.umd.js",
  "module": "./dist/assetStore.es.js",
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    "import": "./dist/assetStore.es.js",
    "require": "./dist/assetStore.umd.js"
  },
  "types": "./types/assetStore.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b ./src/tsconfig.json && vite build",
    "watch": "vite build -w",
    "serve": "vite preview",
    "lint": "eslint src/**/*.{js,ts} --fix",
    "format": "prettier src/**/* --write",
    "test": "mocha -r ts-node/register 'spec/**/*.spec.ts'",
    "cover": "nyc npm t",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shilaca/three-asset-store.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [],
  "author": "shilaca",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/sinon": "^10.0.11",
    "@types/three": "^0.140.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.6.2",
    "sinon": "^14.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8"
  },
  "dependencies": {},
  "peerDependencies": {
    "three": "^0.140.0"
  }
}
